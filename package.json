    "babel-core": "^6.21.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-es2015": "^6.18.0",
    "chai": "^3.5.0",
    "connect": "^3.5.0",
    "eslint": "^3.12.2",
    "express": "^4.14.0",
    "husky": "^0.12.0",
    "istanbul-api": "^1.1.1",
    "istanbul-instrumenter-loader": "^2.0.0",
    "lodash.template": "^4.4.0",
    "memory-fs": "^0.4.1",
    "mocha": "^3.2.0",
    "nyc": "^10.1.2",
    "phantomjs-prebuilt": "^2.1.14",
    "restify": "^4.3.0",
    "staged-files": "^0.1.2",
    "wd": "^1.1.1",
    "webpack": "^1.14.0"
  },
  "config": {
    "port": 8080
  }

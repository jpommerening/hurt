{
  "name": "hurt",
  "version": "0.1.0",
  "description": "HTTP and SPA routing using RFC 6570 URI templates",
  "keywords": [
    "http",
    "router",
    "routing",
    "6570",
    "uri",
    "templates"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jpommerening/hurt"
  },
  "bugs": {
    "url": "https://github.com/jpommerening/hurt/issues"
  },
  "scripts": {
    "prepublish": "babel -d lib src && babel -o index.js src",
    "test": "mocha --compilers js:babel-core/register"
  },
  "dependencies": {
    "eventemitter3": "^2.0.2",
    "uri-templates": "^0.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-es2015": "^6.18.0",
    "chai": "^3.5.0",
    "connect": "^3.5.0",
    "eslint": "^3.12.2",
    "ghooks": "^1.3.2",
    "lodash.template": "^4.4.0",
    "mocha": "^3.2.0",
    "phantomjs-prebuilt": "^2.1.14",
    "staged-files": "^0.1.2",
    "webdriverio": "^4.6.1"
  },
  "config": {
    "port": 8080,
    "ghooks": {
      "pre-push": "npm test",
      "post-merge": "npm install",
      "post-rewrite": "npm install"
    }
  }
